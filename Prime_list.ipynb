   "source": [
    "def prime_list(n):\n",
    "    primes = []\n",
    "    for x in range(n):\n",
    "        dev = [x % y for y in range(1, (x + 1)) if x % y == 0]\n",
    "        if len(dev) == 2:\n",
    "            primes.append(x)\n",
    "    print(primes, end=\"\")"
   ]
